{
  "name": "bemba",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "fast-memoize": "^2.3.2",
    "isomorphic-unfetch": "^2.0.0",
    "lodash": "^4.17.10",
    "next": "^6.0.2",
    "next-redux-wrapper": "^2.0.0-beta.6",
    "nprogress": "^0.2.0",
    "polka": "^0.3.4",
    "randomcolor": "^0.5.3",
    "react": "latest",
    "react-dom": "latest",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-localstorage-simple": "^2.1.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "serve-static": "latest",
    "styled-components": "latest"
  },
  "scripts": {
    "dev": "yarn clean && node server",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "clean": "rm -rf ./node_modules/.cache/babel-loader"
  },
  "now": {
    "name": "bemba",
    "alias": ["bemba.analogic.al"],
    "env": {
      "BEMBA_API_ENDPOINT": "@bemba_api_endpoint"
    }
  },
  "devDependencies": {
    "@zeit/next-bundle-analyzer": "^0.1.1",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.1"
  }
}
